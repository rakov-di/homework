
$svg-common
  display inline-block
  background url("../img/icons-8a67240e.svg") no-repeat


// миксин для конкретной иконки
$icon_calendar
  @extend $svg-common
  background-position: (-128px - (20px - 16px)/2) (-78px - (20px - 16px)/2)
  width 16px
  height 16px

// класс для иконки без использования before
.icon_calendar
  @extend $icon_calendar

// класс для иконки через before
.icon_calendar-before
  &::before
    content ''
    @extend $icon_calendar



